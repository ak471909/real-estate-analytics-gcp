{
  "lifecycle": {
    "rule": [
      {
        "action": {
          "type": "Delete"
        },
        "condition": {
          "age": 30,
          "matchesPrefix": ["raw/"]
        },
        "description": "Delete raw CSV files older than 30 days"
      },
      {
        "action": {
          "type": "Delete"
        },
        "condition": {
          "age": 90
        },
        "description": "Delete all files older than 90 days"
      }
    ]
  }
}